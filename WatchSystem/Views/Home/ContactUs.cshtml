
@model SettingsDto
<!-- breadcrumb start -->
<div class="breadcrumb-section">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <div class="page-title">
                    <h2>@SystemResources.ContactUs</h2>
                </div>
            </div>
            <div class="col-sm-6">
                <nav aria-label="breadcrumb" class="theme-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">@SystemResources.Home</a></li>
                        <li class="breadcrumb-item active">@SystemResources.ContactUs</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<!-- breadcrumb End -->

<!--section start-->
<section class="contact-page section-b-space">
    <div class="container">
        <div class="row section-b-space">
            <div class="col-lg-7 map">
                <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d1605.811957341231!2d25.45976406005396!3d36.3940974010114!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sin!4v1550912388321"
                        allowfullscreen></iframe>
            </div>
            <div class="col-lg-5">
                <div class="contact-right">
                    <ul>
                        <li>
                            <div class="contact-icon">
                          
                                <h6>@SystemResources.ContactUs</h6>
                            </div>
                            <div class="media-body">
                                <p>@Model.ContactNumber</p>
                            </div>
                        </li>
                        <li>
                            <div class="contact-icon">
                                <i class="fa fa-map-marker" aria-hidden="true"></i>
                                <h6>@SystemResources.Address</h6>
                            </div>
                            <div class="media-body">
                                <p>@Model.Address</p>
                               
                            </div>
                        </li>
                        <li>
                            <div class="contact-icon">
                           
                                <h6>@SystemResources.Email</h6>
                            </div>
                            <div class="media-body">
                                <p>@Model.Email</p>
                              
                            </div>
                        </li>
                        <li>
                            <div class="contact-icon">
                                <i class="fa fa-fax" aria-hidden="true"></i>
                                <h6>@SystemResources.Fax</h6>
                            </div>
                            <div class="media-body">
                                <p>@Model.Fax</p>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
   
    </div>
</section>
<!--Section ends-->