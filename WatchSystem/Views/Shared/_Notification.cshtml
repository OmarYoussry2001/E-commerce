<script>
	toastr.options = {
		"closeButton": true,
		"progressBar": true,
		"positionClass": "toast-top-right",
		"timeOut": "5000"
	};
</script>

@if (TempData["SuccessMessage"] != null)
{


	
	<script type="text/javascript">
		toastr.success('@TempData["SuccessMessage"]');

	</script>
}
@if (TempData["ErrorMessage"] != null)
{

		<script type="text/javascript">
		toastr.error('@TempData["ErrorMessage"]');
		</script>

	
}